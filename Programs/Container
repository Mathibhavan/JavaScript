function getContainer(product) {
  let container;
  switch (product) {
    case "Bread":
      container = "bag";
    case "Beer":
    case "Milk":
      container = "bottle";
      break;
    case "Cerials":
      container = "box";
      break;
    case "Eggs":
      container = "carton";
      break;
    case "Candy":
      container = "plastic";
    default:
      container = null;
  }

  return container;
}
console.log(getContainer("Bread"));
